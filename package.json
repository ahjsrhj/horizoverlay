{
  "name": "horizoverlay",
  "description": "A simple horizontal damage meter overlay for Final Fantasy XIV with ACT",
  "version": "2.3.1",
  "author": "Rafael 'BSIDES' Pereira",
  "contributors": [
    "Please check this url for the whole list(https://github.com/ahjsrhj/horizoverlay/graphs/contributors)"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ahjsrhj/horizoverlay.git"
  },
  "bugs": {
    "url": "https://github.com/ahjsrhj/horizoverlay/issues"
  },
  "homepage": "https://ahjsrhj.github.io/horizoverlay",
  "dependencies": {
    "ffxiv-overlay-api": "^3.7.0-beta.1",
    "raven-js": "^3.24.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^3.0.1"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "serve": "^11.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
